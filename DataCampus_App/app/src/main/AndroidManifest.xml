<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.navermap">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.naver.maps.map.CLIENT_ID"
            android:value="2pijo0g7vf"/>

        <activity android:name=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Texi_Activity"
            android:parentActivityName=".MainActivity">
        </activity>

        <activity android:name=".Bus_Activity"
            android:parentActivityName=".MainActivity">
        </activity>

        <activity android:name=".BusStop_Activity"
            android:parentActivityName=".MainActivity">
        </activity>

        <activity android:name=".Bus_Menu.Bus1_Activity"
            android:parentActivityName=".Bus_Activity">
        </activity>
        <activity android:name=".Bus_Menu.Bus2_Activity"
            android:parentActivityName=".Bus_Activity">
        </activity>
        <activity android:name=".Bus_Menu.Bus3_Activity"
            android:parentActivityName=".Bus_Activity">
        </activity>

        <activity android:name=".Map.Map1_1Activity"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".Map.Map1_2Activity"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".Map.Map1_3Activity"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".Map.Map2_1Activity"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".Map.Map3_1Activity"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".Map.MapStation1_Activity"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".Map.MapStation2_Activity"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".Map.MapStation3_Activity"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
    </application>

</manifest>